package miniProject2Harf;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String letter = "a";
		
		switch (letter.toLowerCase()) {
			case "a":
				System.out.println("kalýn harf");
				break;
			case "e":
				System.out.println("ince harf");
				break;
			case "ý":
				System.out.println("kalýn harf");
				break;
			case "i":
				System.out.println("ince harf");
				break;
			case "o":
				System.out.println("kalýn harf");
				break;
			case "ö":
				System.out.println("ince harf");
				break;
			case "u":
				System.out.println("kalýn harf");	
				break;
			case "ü":
				System.out.println("ince harf");	
				break;
			default:
				System.out.println("Sesli harf Giriniz");
		}
		
	}

}
